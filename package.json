{
    "name": "erniebot.ts",
    "type": "module",
    "version": "0.3.2",
    "main": "./dist/index.cjs",
    "module": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
        "import": "./dist/index.js",
        "require": "./dist/index.cjs",
        "types": "./dist/index.d.ts"
    },
    "scripts": {
        "build": "tsup --cjsInterop"
    },
    "dependencies": {
        "streaming-iterables": "^8.0.1",
        "zod": "^3.22.4",
        "zod-to-json-schema": "^3.21.4"
    },
    "devDependencies": {
        "@types/node": "^20.9.0",
        "dotenv": "^16.3.1",
        "tsup": "^7.2.0",
        "typescript": "^5.2.2"
    },
    "tsup": {
        "entry": [
            "src/index.ts"
        ],
        "format": [
            "cjs",
            "esm"
        ],
        "clean": true,
        "dts": true,
        "treeshake": true,
        "shims": true
    },
    "files": [
        "dist/",
        "README.md"
    ]
}